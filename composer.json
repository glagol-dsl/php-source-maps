{
  "name": "bulgaria-php/glagol-dsl-php-source-map",
  "description": "PHP library for reading Source Maps v3",
  "minimum-stability": "dev",
  "prefer-dist": true,
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Yoan-Alexander Grigorov",
      "email": "joan.grigorov@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {"Glagol\\SourceMap\\": "src/"},
    "files": [
      "src/mappings_parse.php",
      "src/sourcemap_loading.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {"GlagolTest\\SourceMap\\": "test/"}
  },
  "require": {
    "php": "~7.1",
    "illuminate/support": "5.*"
  },
  "require-dev": {
    "phpunit/phpunit": "6.*"
  }
}
